{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "useDefineForClassFields": false,
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "baseUrl": "./",
    "skipLibCheck": true,
  },
  "include": ["src/**/*"],
}